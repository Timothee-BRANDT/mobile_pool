{
  "expo": {
    "name": "weatherproj",
    "slug": "weatherproj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.misscoding.googleauthexporeactnative",
      "supportsTablet": true,
      "infoPlist" : {
        "CFBundleURLTypes" : [
            {
                "CFBundleURLSchemes" : [
                    "com.googleusercontent.apps.846524925740-74qiamsh2evf0pm68cke5ik0plaf0qgo"
                ]
            }
        ]
      }
    },
    "android": {
      "package": "com.misscoding.googleauthexporeactnative",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
